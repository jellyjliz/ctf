entrypoint = "main.py"
modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"



[env]
FLASK_APP = "main.py"
FLASK_ENV = "development"

[[ports]]
localPort = 3000
externalPort = 80

